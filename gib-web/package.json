{
  "name": "gib-web",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.1.1",
    "rollup": "^2.38.5",
    "rollup-plugin-scss": "^2.6.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "wasm-pack build --target web --out-dir static/wasm ; rollup -c rollup.config.js ; rm -rf ../gib-server/static/* ; cp -r static/* ../gib-server/static"
  },
  "nodemonConfig": {
    "ignore": [
      "static/wasm"
    ],
    "watch": [
      "src",
      "static",
      "js"
    ],
    "ext": ".rs,.ts,.html,.scss",
    "exec": "yarn build"
  }
}
